<%- include('../partials/header')%>
    <title><%= title %></title>
</head>
<body>

    <%- include('../partials/navbar')%>
    <h1 class="mt-3">Edit profile</h1>
    <form  class="d-grid gap-2 col-6  mx-auto form-edit" method="POST" action="/profiles/<%=category %>/<%= profile.id %>?_method=PATCH" >
        <div>
            <label for="name" class="form-label" >Enter name</label>
            <input type="text" name="name" id="name" value="<%=profile.name %>" class="form-control">
        </div>
        <br>
        <div>
            <label for="<%=keys[2] %>" class="form-label">Enter <%=keys[2] %></label>
            <input type="text" name = "<%=keys[2] %>" id="<%=keys[2] %>" value="<%=profile[keys[2]] %>" class="form-control">
        </div>
        <br>
        <div>
            <label for="age" class="form-label">Enter age</label>
            <input type="number" name="age"    id="age" value="<%=profile.age %>" class="form-control">
        </div>
        <br>
        <div>
            <label for="email" class="form-label">Enter email</label>
            <input type="text" name="email" id="email" value="<%=profile.email %>" class="form-control">
        </div>
        <button type="submit" class="btn btn-warning mt-4 mb-4">Save</button>
        <!-- <button>Save</button> -->
    </form>
</body>
<%- include('../partials/footer') %>
</html>