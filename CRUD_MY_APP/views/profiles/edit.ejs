<%- include('../partials/header')%>
    <title><%= title %></title>
</head>
<body>

    <%- include('../partials/navbar')%>
    <h1>Edit profile</h1>
    <form  class="col-2  mx-auto form-edit" method="POST" action="/profiles/<%=category %>/<%= profile.id %>?_method=PATCH" >
        <div>
            <label for="name" >Enter name</label>
            <input type="text" name="name" id="name" value="<%=profile.name %>">
        </div>
        <br>
        <div>
            <label for="<%=keys[2] %>">Enter <%=keys[2] %></label>
            <input type="text" name = "<%=keys[2] %>" id="<%=keys[2] %>" value="<%=profile[keys[2]] %>">
        </div>
        <br>
        <div>
            <label for="age">Enter age</label>
            <input type="number" name="age"    id="age" value="<%=profile.age %>">
        </div>
        <br>
        <div>
            <label for="email">Enter email</label>
            <input type="text" name="email" id="email" value="<%=profile.email %>">
        </div>
        <button type="submit" class="btn btn-warning mt-4 col-4">Save</button>
        <!-- <button>Save</button> -->
    </form>
</body>
</html>